<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="file://{resources}/styles/dynamic_minimap.css" />
		<include src="file://{resources}/styles/hero_selection.css" />
		<include src="file://{resources}/styles/customchat_selection.css" />
	</styles>
	<scripts>
		<include src="file://{resources}/scripts/util.js" />
		<include src="file://{resources}/scripts/shared_hero_selection_script.js" />
		<include src="file://{resources}/scripts/hero_selection.js" />
		<include src="file://{resources}/scripts/customchat.js" />
	</scripts>
	<snippets>
		<snippet name="HeroesPanel">
			<Panel class="HeroesPanelPage">
				<Panel class="HeroesPanelElement">
					<Panel class="HeroesPanelElementTeam HeroesPanelElementGood" id="HeroesByAttributes_0_good" />
					<Panel class="HeroesPanelElementTeam HeroesPanelElementBad" id="HeroesByAttributes_0_bad" />
				</Panel>
				<Panel class="HeroesPanelElement">
					<Panel class="HeroesPanelElementTeam HeroesPanelElementGood" id="HeroesByAttributes_1_good" />
					<Panel class="HeroesPanelElementTeam HeroesPanelElementBad" id="HeroesByAttributes_1_bad" />
				</Panel>
				<Panel class="HeroesPanelElement">
					<Panel class="HeroesPanelElementTeam HeroesPanelElementGood" id="HeroesByAttributes_2_good" />
					<Panel class="HeroesPanelElementTeam HeroesPanelElementBad" id="HeroesByAttributes_2_bad" />
				</Panel>
			</Panel>
		</snippet>
	</snippets>
	<Panel hittest="false" class="base Shown">
		<Image id="HeroSelectionBackground" hittest="false" />
		<!-- <DOTAScenePanel id="HeroSelectionBackground" map="maps/backgrounds/dashboard_parallax_spring2016.vmap" hittest="false" /> -->
		<Panel hittest="false" id="HeroSelectionBox">
			<!-- <Panel id="SelectedHeroPanel" hittest="false">
				<Panel id="SelectedHeroPreview">
					<Panel id="SelectedHeroScene">
						<DOTAScenePanel style="width: 100%; height: 100%;" unit="npc_dota_hero_abaddon"/>
						
					</Panel>
				</Panel>

				<Panel id="SelectedHeroDescriptionPanel">
					<Label id="SelectedHeroDescriptionText" text="" />
				</Panel>
			</Panel> -->

			<Panel id="HeroListBasePanel">
				<Panel id="HeroListControls">
					<Panel id="HeroListControlsGroup1" class="HeroListControlsGroup">
						<Button class="SmallTextButton" id="SwitchTabButton" onactivate="SwitchTab();">
							<Panel class="SmallTextButtonContainer">
								<Image hittest="false" src="file://{resources}/images/custom_game/hero_selection/hero_swich.png" />
								<Label id="SwitchTabButtonLabel" hittest="false" text="#hero_selection_button_switch" />
							</Panel>
						</Button>
						<Button class="SmallTextButton" id="HeroRandomButton" onactivate="RandomHero();">
							<Panel class="SmallTextButtonContainer">
								<Image hittest="false" src="file://{resources}/images/custom_game/hero_selection/random.png" />
								<Label id="HeroRandomButtonLabel" hittest="false" text="#hero_selection_button_random" />
							</Panel>
						</Button>
					</Panel>
					<Panel id="HeroListControlsGroup2" class="HeroListControlsGroup">
						<Button id="SelectedHeroSelectButton" onactivate="SelectHero();">
							<Label id="SelectedHeroSelectLabel" hittest="false" text="#hero_selection_button_select_hero" />
							<Label id="SelectedHeroSelectHeroName" hittest="false" text="#npc_dota_hero_abaddon" />
						</Button>
					</Panel>
					<Panel id="HeroListControlsGroup3" class="HeroListControlsGroup">
						<Panel id="SelectedHeroAttributesPanel">
							<Panel class="SelectedHeroAttributesElement" style="flow-children: down; height: fit-children; width: fit-children; vertical-align: center; margin-right: 12px;">
								<Image src="file://{resources}/images/custom_game/hud/damage.png" style="horizontal-align: center;"/>
								<Label id="HeroAttributes_damage" class="AttributeValueText" style="horizontal-align: center;" text="0 - 0" />
							</Panel>
							<Panel style="height: 100%; flow-children: down;">
								<Panel class="SelectedHeroAttributesElement">
									<Image src="file://{resources}/images/custom_game/hud/movespeed.png" style="vertical-align: center; padding-left: 6px; margin-right: 5px;"/>
									<Label id="HeroAttributes_speed" class="AttributeValueText" text="0" />
								</Panel>
								<Panel class="SelectedHeroAttributesElement">
									<Label style="color: white; align: center center; margin-left: 5px; font-size: 22px; horizontal-align: left;" text="BAT: " />
									<Label id="HeroAttributes_bat" class="AttributeValueText" text="0" />
								</Panel>
								<Panel class="SelectedHeroAttributesElement">
									<Image src="file://{resources}/images/custom_game/hud/armor.png" style="vertical-align: center; padding-left: 4px;"/>
									<Label id="HeroAttributes_armor" style="margin-left: 5px;" class="AttributeValueText" text="0" />
								</Panel>
							</Panel>
							<Panel style="width: 128px; height: 100%; flow-children: down;">
								<Panel class="SelectedHeroAttributesElement">
									<Panel class="DotaAttributePic" id="DotaAttributePic_1" />
									<Label id="HeroAttributes_1" class="AttributeValueText" text="0 + 0.0" />
								</Panel>
								<Panel class="SelectedHeroAttributesElement">
									<Label class="DotaAttributePic" id="DotaAttributePic_2" text="â–³" />
									<Label id="HeroAttributes_2" class="AttributeValueText" text="0 + 0.0" />
								</Panel>
								<Panel class="SelectedHeroAttributesElement">
									<Panel class="DotaAttributePic" id="DotaAttributePic_3" />
									<Label id="HeroAttributes_3" class="AttributeValueText" text="0 + 0.0" />
								</Panel>
							</Panel>
						</Panel>
						<!-- <Button class="SmallTextButton" onactivate="RandomHero();">
							<Panel class="SmallTextButtonContainer">
								<Image hittest="false" src="file://{resources}/images/custom_game/hero_selection/random.png" />
								<Label id="HeroRandomButtonLabel" hittest="false" text="#hero_selection_button_random" />
							</Panel>
						</Button> -->
						<!-- <Button id="HeroRandomButton" onactivate="RandomHero();">
							<Label id="HeroRandomButtonLabel" hittest="false" text="#hero_selection_button_random" />
						</Button> -->
					</Panel>
				</Panel>
				<Panel id="AbilitiesPanel">
					<Panel class="AbilitiesLineSplitter" />
					<Panel class="AbilitiesLineSplitter" style="horizontal-align: right;" />
					<TextEntry id="HeroSearchTextEntry" oninputsubmit="SearchHero();" placeholder="#hero_selection_button_search"/>
					<Panel id="SelectedHeroAbilitiesPanelInner" />
				</Panel>
				<Panel id="HeroListBasePanelHost">
					<Panel id="HeroListPanel" />
					<Panel id="HeroListScenes" />
				</Panel>
			</Panel>

			<Panel id="SelectionStatusPanel" >
				<Panel id="GameModeInfoPanel" >
					<Panel id="ExitButton" onactivate="$.DispatchEvent('DOTAShowDashboard')"/>
					<Label id="HeroSelectionTimer" text="0" />
					<Label id="GameModeInfoGamemodeLabel" text="#hero_selection_mode_all_pick" />
				</Panel>
				<Panel id="TeamSelectionStatusPanel" />
			</Panel>

			<Panel id="ChatAndMapPanel">
				<!-- class="Debug" -->
				<Panel id="MinimapPanel" class="">
					<Image id="MinimapImage" src="file://{images}/custom_game/hero_selection/5v5.tga">
						<Panel class="MinimapDynamicIcons" />
						<Panel class="MinimapSpawnBoxes">
							<!-- Radiant -->
								<!-- easy -->
									<Panel id="MinimapSpawnBox_2_1_0" onactivate="OnMinimapClickSpawnBox(2, 1, 0)" class="MinimapSpawnBox" style="position: 96px 160px 0;"/>
								<!-- medium -->
									<Panel id="MinimapSpawnBox_2_2_0" onactivate="OnMinimapClickSpawnBox(2, 2, 0)" class="MinimapSpawnBox" style="position: 15px 198px 0;"/>
									<Panel id="MinimapSpawnBox_2_2_1" onactivate="OnMinimapClickSpawnBox(2, 2, 1)" class="MinimapSpawnBox" style="position: 50px 198px 0;"/>
									<Panel id="MinimapSpawnBox_2_2_2" onactivate="OnMinimapClickSpawnBox(2, 2, 2)" class="MinimapSpawnBox" style="position: 53px 74px 0;"/>
									<Panel id="MinimapSpawnBox_2_2_3" onactivate="OnMinimapClickSpawnBox(2, 2, 3)" class="MinimapSpawnBox" style="position: 53px 123px 0;"/>
								<!-- hard -->
									<Panel id="MinimapSpawnBox_2_3_0" onactivate="OnMinimapClickSpawnBox(2, 3, 0)" class="MinimapSpawnBox" style="position: 94px 224px 0;"/>
									<Panel id="MinimapSpawnBox_2_3_1" onactivate="OnMinimapClickSpawnBox(2, 3, 1)" class="MinimapSpawnBox" style="position: 100px 89px 0;"/>
							<!-- Dire -->
								<!-- easy -->
									<Panel id="MinimapSpawnBox_3_1_0" onactivate="OnMinimapClickSpawnBox(3, 1, 0)" class="MinimapSpawnBox" style="position: 220px 160px 0;"/>
								<!-- medium -->
									<Panel id="MinimapSpawnBox_3_2_0" onactivate="OnMinimapClickSpawnBox(3, 2, 0)" class="MinimapSpawnBox" style="position: 306px 202px 0;"/>
									<Panel id="MinimapSpawnBox_3_2_1" onactivate="OnMinimapClickSpawnBox(3, 2, 1)" class="MinimapSpawnBox" style="position: 272px 202px 0;"/>
									<Panel id="MinimapSpawnBox_3_2_2" onactivate="OnMinimapClickSpawnBox(3, 2, 2)" class="MinimapSpawnBox" style="position: 268px 116px 0;"/>
									<Panel id="MinimapSpawnBox_3_2_3" onactivate="OnMinimapClickSpawnBox(3, 2, 3)" class="MinimapSpawnBox" style="position: 268px 75px 0;"/>
								<!-- hard -->
									<Panel id="MinimapSpawnBox_3_3_0" onactivate="OnMinimapClickSpawnBox(3, 3, 0)" class="MinimapSpawnBox" style="position: 225px 224px 0;"/>
									<Panel id="MinimapSpawnBox_3_3_1" onactivate="OnMinimapClickSpawnBox(3, 3, 1)" class="MinimapSpawnBox" style="position: 220px 89px 0;"/>
						</Panel>
					</Image>
				</Panel>
				<Panel id="SelectionChatBox">
					<Panel id="SelectionChat">
						<Panel id="SelectionChatMessagesPanel">
							<Panel id="SelectionChatMessages" />
						</Panel>
						<Panel id="SelectionChatEntryPanel">
							<TextEntry id="SelectionChatEntry" maxchars="128" oninputsubmit="ChatSendMessage()" placeholder="#chat_placeholder_send_message"/>
						</Panel>
					</Panel>
				</Panel>
			</Panel>
		</Panel>

		<Panel id="HeroSelectionPrecacheBase">
			<Panel id="PrecacheLoadingMessagePanel">
				<Label id="PrecacheLoadingMessageLabel" text="#hero_selection_loading_message"/>
			</Panel>
			<Panel id="PrecacheTeamContainer">
				<Panel id="PrecacheTeamEntryTeam2" class="PrecacheTeamEntry" />
				<Panel id="PrecacheTeamEntryTeam3" class="PrecacheTeamEntry" />
			</Panel>
		</Panel>
		<Panel id="GamePausedOverlay" hittest="true">
			<Panel style="align:center center;flow-children:down;">
				<Label text="#hero_selection_paused_label" id="GamePausedTitle" />
				<Label text="#hero_selection_paused_description" id="GamePausedDescription" />
			</Panel>
		</Panel>
	</Panel>
</root>
