					<Panel id="HeroPreviewTabs">
						<RadioButton class="HeroPreviewTab" group="preview_tabs" selected="true" text="#DOTA_HeroLoadout_Loadout" onactivate="ShowHeroPreviewTab('LoadoutTabContents')" />
