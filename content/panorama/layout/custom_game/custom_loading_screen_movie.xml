<root>
	<styles>
		<include src="file://{resources}/styles/custom_loading_screen_movie.css" />
	</styles>
	<scripts>
		<include src="file://{resources}/scripts/custom_loading_screen_movie.js" />
	</scripts>
	<Panel class="base">
		<Image id="UVAO" src="file://{images}/custom_game/loading_screen/uvao_head.png" />
		<Image id="Arthas" src="file://{images}/custom_game/loading_screen/arthas.png">
			<Image id="blackhole1" class="Blackhole" src="file://{images}/custom_game/loading_screen/pnchi.png" />
			<Image id="blackhole2" class="Blackhole" src="file://{images}/custom_game/loading_screen/pnchi.png" />
		</Image>
		<Image id="Enigma" src="file://{images}/custom_game/loading_screen/enigma.png" />
		<!-- <Image id="Lina" src="file://{images}/custom_game/loading_screen/lina.png" /> -->
		<Image id="EnigmasBlackhole" src="file://{images}/custom_game/loading_screen/black-hole.png" />
		<Image id="Logo_aa" src="file://{images}/custom_game/loading_screen/logo_aa.png" />
		<Image id="Logo_jesus" src="file://{images}/custom_game/loading_screen/logo_jesus.png" />
		<Image id="Logo_satan" src="file://{images}/custom_game/loading_screen/logo_satan.png" />
		<Panel id="Label_black_panel">
			<Label id="Label_black" text="BLACK" />
		</Panel>
		<Panel id="StarsBox">
			<Label id="Label_star" text="STAR" />
			<Image id="star1" class="star" />
			<Image id="star2" class="star" />
			<Image id="star3" class="star" />
			<Image id="star4" class="star" />
			<Image id="star5" class="star" />
		</Panel>
		<Image id="loading_bar" src="file://{images}/custom_game/loading_screen/loading_bar_full.png" />
		<Panel id="GoalVotePanel">
			<Label id="GoalVoteLabel" text="#kill_goal_tooltip_vote_title"/>
			<Panel id="GoalVotePanelTextEntry">
				<TextEntry id="GoalVoteTextEntry" maxchars="3" />
				<Label id="GoalVoteMinValue" text=".." />
				<Label id="GoalVoteMaxValue" text=".." />
			</Panel>
			<Button id="GoalVoteButton" onactivate="SendKillGoalVote()">
				<Label id="GoalVoteButtonLabel" hittest="false" text="#kill_goal_tooltip_vote_submit"/>
			</Button>
		</Panel>
	</Panel>
</root>