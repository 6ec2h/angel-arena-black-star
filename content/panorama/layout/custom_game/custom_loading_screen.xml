	<snippets>
		<snippet name="OptionVoting">
			<Panel class="OptionVoting">
				<Label id="OptionVotingTitle" text="{s:title}"/>
				<Panel id="OptionVotingVariants" />
			</Panel>
		</snippet>
		<snippet name="TopPlayer">
			<Panel class="TopPlayer">
				<DOTAAvatarImage id="PlayerAvatar" />
				<DOTAUserName id="PlayerName" />
				<Label id="PlayerRating" text="{d:rating}"/>
			</Panel>
		</snippet>
	</snippets>
