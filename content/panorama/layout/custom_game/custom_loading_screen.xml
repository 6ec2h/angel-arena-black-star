	<script>
		function CheckStartable() {
			var player = Game.GetLocalPlayerInfo()
			if (player == null) {
				$.Schedule(0.2, CheckStartable)
			} else {
				$("#loading_base").RemoveAndDeleteChildren()
				$("#movie_base").BLoadLayout("file://{resources}/layout/custom_game/custom_loading_screen_movie.xml", true, true)
			}
		}
		(function() {
			$.Schedule(0.2, CheckStartable)
		})()
	</script>
